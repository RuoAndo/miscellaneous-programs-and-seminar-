16375:@@ -313,3 +313,80 @@
16379:+
16380:+static char unamebuf[BUFSIZ];
16381:+static char *unamep = NULL;
16382:+
16383:+static void
16384:+getuname(void) {
16385:+	DWORD fvilen;
16386:+	char *fvi;
16387:+	VS_FIXEDFILEINFO *ffi;
16388:+	UINT ffilen;
16389:+	SYSTEM_INFO sysinfo;
16390:+	char *arch;
16391:+
16392:+	fvi = NULL;
16393:+	fvilen = GetFileVersionInfoSize("kernel32.dll", 0);
16394:+	if (fvilen == 0) {
16395:+		goto err;
16396:+	}
16397:+	fvi = (char *)malloc(fvilen);
16398:+	if (fvi == NULL) {
16399:+		goto err;
16400:+	}
16401:+	memset(fvi, 0, fvilen);
16402:+	if (GetFileVersionInfo("kernel32.dll", 0, fvilen, fvi) == 0) {
16403:+		goto err;
16404:+	}
16405:+	ffi = NULL;
16406:+	ffilen = 0;
16407:+	if ((VerQueryValue(fvi, "\\", &ffi, &ffilen) == 0) ||
16408:+	    (ffi == NULL) || (ffilen == 0)) {
16409:+		goto err;
16410:+	}
16411:+	memset(&sysinfo, 0, sizeof(sysinfo));
16412:+	GetSystemInfo(&sysinfo);
16413:+	switch (sysinfo.wProcessorArchitecture) {
16414:+	case PROCESSOR_ARCHITECTURE_INTEL:
16415:+		arch = "x86";
16416:+		break;
16417:+	case PROCESSOR_ARCHITECTURE_ARM:
16418:+		arch = "arm";
16419:+		break;
16420:+	case PROCESSOR_ARCHITECTURE_IA64:
16421:+		arch = "ia64";
16422:+		break;
16423:+	case PROCESSOR_ARCHITECTURE_AMD64:
16424:+		arch = "x64";
16425:+		break;
16426:+	default:
16427:+		arch = "unknown architecture";
16428:+		break;
16429:+	}
16430:+
16431:+	snprintf(unamebuf, sizeof(unamebuf),
16432:+		 "Windows %d %d build %d %d for %s\n",
16433:+		 (ffi->dwProductVersionMS >> 16) & 0xffff,
16434:+		 ffi->dwProductVersionMS & 0xffff,
16435:+		 (ffi->dwProductVersionLS >> 16) & 0xffff,
16436:+		 ffi->dwProductVersionLS & 0xffff,
16437:+		 arch);
16438:+
16439:+    err:
16440:+	if (fvi != NULL) {
16441:+		free(fvi);
16442:+	}
16443:+	unamep = unamebuf;
16444:+}
16445:+
16446:+/*
16447:+ * GetVersionEx() returns 6.2 (aka Windows 8.1) since it was obsoleted
16448:+ * so we had to switch to the recommended way to get the Windows version.
16449:+ */
16450:+char *
16451:+ns_os_uname(void) {
16452:+	if (unamep == NULL)
16453:+		getuname();
16454:+	return (unamep);
16455:+}
