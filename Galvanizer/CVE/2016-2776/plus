11866:+static isc_result_t
11867:+getcommand(isc_lex_t *lex, char **cmdp) {
11868:+	isc_result_t result;
11869:+	isc_token_t token;
11870:+
11871:+	REQUIRE(cmdp != NULL && *cmdp == NULL);
11872:+
11873:+	result = isc_lex_gettoken(lex, ISC_LEXOPT_EOF, &token);
11874:+	if (result != ISC_R_SUCCESS)
11875:+		return (result);
11876:+
11877:+	isc_lex_ungettoken(lex, &token);
11878:+
11879:+	if (token.type != isc_tokentype_string)
11880:+		return (ISC_R_FAILURE);
11881:+
11882:+	*cmdp = token.value.as_textregion.base;
11883:+
11884:+	return (ISC_R_SUCCESS);
11885:+}
11886:+
