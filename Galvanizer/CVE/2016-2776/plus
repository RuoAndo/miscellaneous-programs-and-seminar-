14978:+	if (client != NULL && client->query.qname != NULL) {
14979:+		if (isc_log_wouldlog(ns_g_lctx, level)) {
14980:+			char qbuf[DNS_NAME_FORMATSIZE];
14981:+			char tbuf[DNS_RDATATYPE_FORMATSIZE];
14982:+			dns_name_format(client->query.qname,
14983:+					qbuf, sizeof(qbuf));
14984:+			dns_rdatatype_format(client->query.qtype,
14985:+					     tbuf, sizeof(tbuf));
14986:+			isc_log_write(ns_g_lctx,
14987:+				      NS_LOGCATEGORY_CLIENT,
14988:+				      NS_LOGMODULE_QUERY, level,
14989:+				      "query client=%p thread=0x%lx "
14990:+				      "(%s/%s): %s",
14991:+				      client,
14992:+				      (unsigned long) isc_thread_self(),
14993:+				      qbuf, tbuf, message);
14994:+		}
14995:+	 } else {
14996:+		isc_log_write(ns_g_lctx,
14997:+			      NS_LOGCATEGORY_CLIENT,
14998:+			      NS_LOGMODULE_QUERY, level,
14999:+			      "query client=%p thread=0x%lx "
15000:+			      "(<unknown-query>): %s",
15001:+			      client,
15002:+			      (unsigned long) isc_thread_self(),
15003:+			      message);
15004:+	}
15005:+}
15006:+#define CTRACE(l,m)	  client_trace(client, l, m)
