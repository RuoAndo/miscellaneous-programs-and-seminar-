36627:+  p
36628:+  q
36629:+}
36630:+/@datadir@/p
36631:+/@docdir@/p
36632:+/@infodir@/p
36633:+/@localedir@/p
36634:+/@mandir@/p'
36635:+case `eval "sed -n \"\$ac_sed_dataroot\" $ac_file_inputs"` in
36636:+*datarootdir*) ac_datarootdir_seen=yes;;
36637:+*@datadir@*|*@docdir@*|*@infodir@*|*@localedir@*|*@mandir@*)
36638:+  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: $ac_file_inputs seems to ignore the --datarootdir setting" >&5
36639:+$as_echo "$as_me: WARNING: $ac_file_inputs seems to ignore the --datarootdir setting" >&2;}
36640:+_ACEOF
36641:+cat >>$CONFIG_STATUS <<_ACEOF || ac_write_fail=1
36642:+  ac_datarootdir_hack='
36643:+  s&@datadir@&$datadir&g
36644:+  s&@docdir@&$docdir&g
36645:+  s&@infodir@&$infodir&g
36646:+  s&@localedir@&$localedir&g
36647:+  s&@mandir@&$mandir&g
36648:+  s&\\\${datarootdir}&$datarootdir&g' ;;
36649:+esac
36653:+
36654:+# Neutralize VPATH when `$srcdir' = `.'.
36655:+# Shell code in configure.ac might set extrasub.
36656:+# FIXME: do we really want to maintain this feature?
36657:+cat >>$CONFIG_STATUS <<_ACEOF || ac_write_fail=1
36658:+ac_sed_extra="$ac_vpsub
36662:+cat >>$CONFIG_STATUS <<\_ACEOF || ac_write_fail=1
36691:+s|@configure_input@|$ac_sed_conf_input|;t t
36692:+s&@top_builddir@&$ac_top_builddir_sub&;t t
36693:+s&@top_build_prefix@&$ac_top_build_prefix&;t t
36694:+s&@srcdir@&$ac_srcdir&;t t
36695:+s&@abs_srcdir@&$ac_abs_srcdir&;t t
36696:+s&@top_srcdir@&$ac_top_srcdir&;t t
36697:+s&@abs_top_srcdir@&$ac_abs_top_srcdir&;t t
36698:+s&@builddir@&$ac_builddir&;t t
36699:+s&@abs_builddir@&$ac_abs_builddir&;t t
36700:+s&@abs_top_builddir@&$ac_abs_top_builddir&;t t
36701:+s&@INSTALL@&$ac_INSTALL&;t t
36702:+$ac_datarootdir_hack
36703:+"
36704:+eval sed \"\$ac_sed_extra\" "$ac_file_inputs" | $AWK -f "$ac_tmp/subs.awk" \
36705:+  >$ac_tmp/out || as_fn_error $? "could not create $ac_file" "$LINENO" 5
36706:+
36707:+test -z "$ac_datarootdir_hack$ac_datarootdir_seen" &&
36708:+  { ac_out=`sed -n '/\${datarootdir}/p' "$ac_tmp/out"`; test -n "$ac_out"; } &&
36709:+  { ac_out=`sed -n '/^[	 ]*datarootdir[	 ]*:*=/p' \
36710:+      "$ac_tmp/out"`; test -z "$ac_out"; } &&
36711:+  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: $ac_file contains a reference to the variable \`datarootdir'
36712:+which seems to be undefined.  Please make sure it is defined" >&5
36713:+$as_echo "$as_me: WARNING: $ac_file contains a reference to the variable \`datarootdir'
36714:+which seems to be undefined.  Please make sure it is defined" >&2;}
36732:+  rm -f "$ac_tmp/stdin"
36883:+  -) cat "$ac_tmp/out" && rm -f "$ac_tmp/out";;
36884:+  *) rm -f "$ac_file" && mv "$ac_tmp/out" "$ac_file";;
36885:+  esac \
36886:+  || as_fn_error $? "could not create $ac_file" "$LINENO" 5
36887:+ ;;
36888:+  :H)
36889:+  #
36890:+  # CONFIG_HEADER
36891:+  #
36896:+    {
36897:+      $as_echo "/* $configure_input  */" \
36898:+      && eval '$AWK -f "$ac_tmp/defines.awk"' "$ac_file_inputs"
36899:+    } >"$ac_tmp/config.h" \
36900:+      || as_fn_error $? "could not create $ac_file" "$LINENO" 5
36901:+    if diff "$ac_file" "$ac_tmp/config.h" >/dev/null 2>&1; then
36902:+      { $as_echo "$as_me:${as_lineno-$LINENO}: $ac_file is unchanged" >&5
36903:+$as_echo "$as_me: $ac_file is unchanged" >&6;}
