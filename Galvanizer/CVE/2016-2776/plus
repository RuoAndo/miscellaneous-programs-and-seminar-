28042:+# as_fn_mkdir_p
28043:+# -------------
28044:+# Create "$as_dir" as a directory, including parents if necessary.
28045:+as_fn_mkdir_p ()
28046:+{
28047:+
28048:+  case $as_dir in #(
28049:+  -*) as_dir=./$as_dir;;
28050:+  esac
28051:+  test -d "$as_dir" || eval $as_mkdir_p || {
28052:+    as_dirs=
28053:+    while :; do
28054:+      case $as_dir in #(
28055:+      *\'*) as_qdir=`$as_echo "$as_dir" | sed "s/'/'\\\\\\\\''/g"`;; #'(
28056:+      *) as_qdir=$as_dir;;
28057:+      esac
28058:+      as_dirs="'$as_qdir' $as_dirs"
28059:+      as_dir=`$as_dirname -- "$as_dir" ||
28060:+$as_expr X"$as_dir" : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
28061:+	 X"$as_dir" : 'X\(//\)[^/]' \| \
28062:+	 X"$as_dir" : 'X\(//\)$' \| \
28063:+	 X"$as_dir" : 'X\(/\)' \| . 2>/dev/null ||
28064:+$as_echo X"$as_dir" |
28065:+    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
28066:+	    s//\1/
28067:+	    q
