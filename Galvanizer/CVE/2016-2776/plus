35566:+      case $as_dir in #(
35567:+      *\'*) as_qdir=`$as_echo "$as_dir" | sed "s/'/'\\\\\\\\''/g"`;; #'(
35568:+      *) as_qdir=$as_dir;;
35569:+      esac
35570:+      as_dirs="'$as_qdir' $as_dirs"
35571:+      as_dir=`$as_dirname -- "$as_dir" ||
35572:+$as_expr X"$as_dir" : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
35573:+	 X"$as_dir" : 'X\(//\)[^/]' \| \
