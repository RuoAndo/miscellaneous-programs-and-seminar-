21729:+#ifdef ISC_PLATFORM_HAVEATOMICSTORE
21730:+static isc_int32_t store_32;
21731:+
21732:+static void
21733:+do_store(isc_task_t *task, isc_event_t *ev) {
21734:+	counter_t *state = (counter_t *)ev->ev_arg;
21735:+	int i;
21736:+	isc_uint8_t r;
21737:+	isc_uint32_t val;
21738:+
21739:+	r = random() % 256;
21740:+	val = (r << 24) | (r << 16) | (r << 8) | r;
21741:+
21742:+	for (i = 0 ; i < COUNTS_PER_ITERATION ; i++) {
21743:+		isc_atomic_store(&store_32, val);
21744:+	}
21745:+
21746:+	state->iteration++;
21747:+	if (state->iteration < ITERATIONS) {
21748:+		isc_task_send(task, &ev);
21749:+	} else {
21750:+		isc_event_free(&ev);
21751:+	}
21752:+}
21753:+
21754:+static void
21755:+test_atomic_store() {
21756:+	int test_result;
21757:+	isc_task_t *tasks[TASKS];
21758:+	isc_event_t *event;
21759:+	int i;
21760:+	isc_uint8_t r;
21761:+	isc_uint32_t val;
21762:+
21763:+	t_assert("test_atomic_store", 1, T_REQUIRED, "%s",
21764:+		 "ensure that isc_atomic_store() works.");
21765:+
21766:+	setup();
21767:+
21768:+	memset(counters, 0, sizeof(counters));
21769:+	store_32 = 0;
21770:+
21771:+	/*
21772:+	 * Create our tasks, and allocate an event to get the counters
21773:+	 * going.
21774:+	 */
21775:+	for (i = 0 ; i < TASKS ; i++) {
21776:+		tasks[i] = NULL;
21777:+		CHECK(isc_task_create(task_manager, 0, &tasks[i]));
21778:+		event = isc_event_allocate(mctx, NULL, 1000, do_store,
21779:+					   &counters[i],
21780:+					   sizeof(struct isc_event));
21781:+		isc_task_sendanddetach(&tasks[i], &event);
21782:+	}
21783:+
21784:+	teardown();
21785:+
21786:+	test_result = T_PASS;
21787:+	r = store_32 & 0xff;
21788:+	val = (r << 24) | (r << 16) | (r << 8) | r;
21789:+	t_info("32-bit store 0x%x, expected 0x%x\n",
21790:+	       (isc_uint32_t) store_32, val);
21791:+	if ((isc_uint32_t) store_32 != val)
21792:+		test_result = T_FAIL;
21793:+	t_result(test_result);
21794:+
21795:+	store_32 = 0;
21796:+}
21797:+#endif
21798:+
21799:+#if defined(ISC_PLATFORM_HAVEATOMICSTOREQ)
21800:+static isc_int64_t store_64;
21801:+
21802:+static void
21803:+do_storeq(isc_task_t *task, isc_event_t *ev) {
21804:+	counter_t *state = (counter_t *)ev->ev_arg;
21805:+	int i;
21806:+	isc_uint8_t r;
21807:+	isc_uint64_t val;
21808:+
21809:+	r = random() % 256;
21810:+	val = (((isc_uint64_t) r << 24) |
21811:+	       ((isc_uint64_t) r << 16) |
21812:+	       ((isc_uint64_t) r << 8) |
21813:+	       (isc_uint64_t) r);
21814:+	val |= ((isc_uint64_t) val << 32);
21815:+
21816:+	for (i = 0 ; i < COUNTS_PER_ITERATION ; i++) {
21817:+		isc_atomic_storeq(&store_64, val);
21818:+	}
21819:+
21820:+	state->iteration++;
21821:+	if (state->iteration < ITERATIONS) {
21822:+		isc_task_send(task, &ev);
21823:+	} else {
21824:+		isc_event_free(&ev);
21825:+	}
21826:+}
21827:+
21828:+static void
21829:+test_atomic_storeq() {
21830:+	int test_result;
21831:+	isc_task_t *tasks[TASKS];
21832:+	isc_event_t *event;
21833:+	int i;
21834:+	isc_uint8_t r;
21835:+	isc_uint64_t val;
21836:+
21837:+	t_assert("test_atomic_storeq", 1, T_REQUIRED, "%s",
21838:+		 "ensure that isc_atomic_storeq() works.");
21839:+
21840:+	setup();
21841:+
21842:+	memset(counters, 0, sizeof(counters));
21843:+	store_64 = 0;
21844:+
21845:+	/*
21846:+	 * Create our tasks, and allocate an event to get the counters going.
21847:+	 */
