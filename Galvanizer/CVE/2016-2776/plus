68422:+isc_boolean_t
68423:+dns_name_isrfc1918(const dns_name_t *name) {
68424:+	size_t i;
68425:+
68426:+	for (i = 0; i < (sizeof(rfc1918names)/sizeof(*rfc1918names)); i++)
