74359:+#ifdef ISC_PLATFORM_USETHREADS
74360:+#ifdef DNS_BENCHMARK_TESTS
74361:+
74362:+/*
74363:+ * XXXMUKS: Don't delete this code. It is useful in benchmarking the
74364:+ * name parser, but we don't require it as part of the unit test runs.
74365:+ */
74366:+
74367:+ATF_TC(benchmark);
74368:+ATF_TC_HEAD(benchmark, tc) {
74369:+	atf_tc_set_md_var(tc, "descr",
74370:+			  "Benchmark dns_name_fromwire() implementation");
74371:+}
74372:+
74373:+static void *
74374:+fromwire_thread(void *arg) {
74375:+	unsigned int maxval = 32000000;
74376:+	uint8_t data[] = {
74377:+		3, 'w', 'w', 'w',
74378:+		7, 'e', 'x', 'a', 'm', 'p', 'l', 'e',
74379:+		7, 'i', 'n', 'v', 'a', 'l', 'i', 'd',
74380:+		0
74381:+	};
74382:+	unsigned char output_data[DNS_NAME_MAXWIRE];
74383:+	isc_buffer_t source, target;
74384:+	unsigned int i;
74385:+	dns_decompress_t dctx;
74386:+
74387:+	UNUSED(arg);
74388:+
74389:+	dns_decompress_init(&dctx, -1, DNS_DECOMPRESS_STRICT);
74390:+	dns_decompress_setmethods(&dctx, DNS_COMPRESS_NONE);
74391:+
74392:+	isc_buffer_init(&source, data, sizeof(data));
74393:+	isc_buffer_add(&source, sizeof(data));
74394:+	isc_buffer_init(&target, output_data, sizeof(output_data));
74395:+
74396:+	/* Parse 32 million names in each thread */
74397:+	for (i = 0; i < maxval; i++) {
74398:+		dns_name_t name;
74399:+
74400:+		isc_buffer_clear(&source);
74401:+		isc_buffer_clear(&target);
74402:+		isc_buffer_add(&source, sizeof(data));
74403:+		isc_buffer_setactive(&source, sizeof(data));
74404:+
74405:+		dns_name_init(&name, NULL);
74406:+		(void) dns_name_fromwire(&name, &source, &dctx, 0, &target);
74407:+	}
74408:+
74409:+	return (NULL);
74410:+}
74411:+
74412:+ATF_TC_BODY(benchmark, tc) {
74413:+	isc_result_t result;
74414:+	unsigned int i;
74415:+	isc_time_t ts1, ts2;
74416:+	double t;
74417:+	unsigned int nthreads;
74418:+	isc_thread_t threads[32];
74419:+
74420:+	UNUSED(tc);
74421:+
74422:+	debug_mem_record = ISC_FALSE;
74423:+
74424:+	result = dns_test_begin(NULL, ISC_TRUE);
74425:+	ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74426:+
74427:+	result = isc_time_now(&ts1);
74428:+	ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74429:+
74430:+	nthreads = ISC_MIN(isc_os_ncpus(), 32);
74431:+	nthreads = ISC_MAX(nthreads, 1);
74432:+	for (i = 0; i < nthreads; i++) {
74433:+		result = isc_thread_create(fromwire_thread, NULL, &threads[i]);
74434:+		ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74435:+	}
74436:+
74437:+	for (i = 0; i < nthreads; i++) {
74438:+		result = isc_thread_join(threads[i], NULL);
74439:+		ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74440:+	}
74441:+
74442:+	result = isc_time_now(&ts2);
74443:+	ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74444:+
74445:+	t = isc_time_microdiff(&ts2, &ts1);
74446:+
74447:+	printf("%u dns_name_fromwire() calls, %f seconds, %f calls/second\n",
74448:+	       nthreads * 32000000, t / 1000000.0,
74449:+	       (nthreads * 32000000) / (t / 1000000.0));
74450:+
74451:+	dns_test_end();
74452:+}
74453:+
74454:+#endif /* DNS_BENCHMARK_TESTS */
74455:+#endif /* ISC_PLATFORM_USETHREADS */
74456:+
74462:+#ifdef ISC_PLATFORM_USETHREADS
74463:+#ifdef DNS_BENCHMARK_TESTS
74464:+	ATF_TP_ADD_TC(tp, benchmark);
74465:+#endif /* DNS_BENCHMARK_TESTS */
74466:+#endif /* ISC_PLATFORM_USETHREADS */
74471:--- bind-9.9.8/lib/dns/tests/rdata_test.c	2015-09-09 11:23:50.000000000 +0900
