16242:+	}
16243:+
16244:+	snprintf(unamebuf, sizeof(unamebuf),
16245:+		 "%s %s %s %s",
16246:+		 uts.sysname, uts.machine, uts.release, uts.version);
16247:+#else
16248:+	strcpy(unamebuf, "unknown architecture");
16249:+#endif
16250:+	unamep = unamebuf;
16251:+}
