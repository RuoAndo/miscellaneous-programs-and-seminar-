15351:+next_token(isc_lex_t *lex, isc_buffer_t *text) {
15352:+	isc_result_t result;
15353:+	isc_token_t token;
15361:+	token.type = isc_tokentype_unknown;
15362:+	result = isc_lex_gettoken(lex, ISC_LEXOPT_EOF|ISC_LEXOPT_QSTRING,
15363:+				  &token);
15364:+
15365:+	switch (result) {
15366:+	case ISC_R_NOMORE:
15367:+		(void) isc_lex_close(lex);
15368:+		break;
15369:+	case ISC_R_SUCCESS:
15370:+		if (token.type == isc_tokentype_eof)
15371:+			(void) isc_lex_close(lex);
15372:+		break;
15373:+	case ISC_R_NOSPACE:
15374:+		if (text != NULL) {
15375:+			(void) putstr(text, "token too large");
15376:+			(void) putnull(text);
15377:+		}
15378:+		return (NULL);
15379:+	default:
15380:+		if (text != NULL) {
15381:+			(void) putstr(text, isc_result_totext(result));
15382:+			(void) putnull(text);
15383:+		}
15384:+		return (NULL);
15385:+	}
15386:+
15387:+	if (token.type == isc_tokentype_string ||
15388:+	    token.type == isc_tokentype_qstring)
15389:+		return (token.value.as_textregion.base);
15390:+
15391:+	return (NULL);
