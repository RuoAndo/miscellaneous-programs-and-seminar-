75822:@@ -360,6 +360,73 @@
75826:+/*
75827:+ * Look for the given header in headers.
75828:+ * If value is specified look for it terminated with a character in eov.
75829:+ */
75830:+static isc_boolean_t
75831:+have_header(isc_httpd_t *httpd, const char *header, const char *value,
75832:+	    const char *eov)
75833:+{
75834:+	char *cr, *nl, *h;
75835:+	size_t hlen, vlen = 0;
75836:+
75837:+	h = httpd->headers;
75838:+	hlen = strlen(header);
75839:+	if (value != NULL) {
75840:+		INSIST(eov != NULL);
75841:+		vlen = strlen(value);
75842:+	}
75843:+
75844:+	for (;;) {
75845:+		if (strncasecmp(h, header, hlen) != 0) {
75846:+			/*
75847:+			 * Skip to next line;
75848:+			 */
75849:+			cr = strchr(h, '\r');
75850:+			if (cr != NULL && cr[1] == '\n')
75851:+				cr++;
75852:+			nl = strchr(h, '\n');
75853:+
75854:+			/* last header? */
75855:+			h = cr;
75856:+			if (h == NULL || (nl != NULL && nl < h))
75857:+				h = nl;
75858:+			if (h == NULL)
75859:+				return (ISC_FALSE);
75860:+			h++;
75861:+			continue;
75862:+		}
75863:+
75864:+		if (value == NULL)
75865:+			return (ISC_TRUE);
75866:+
75867:+		/*
75868:+		 * Skip optional leading white space.
75869:+		 */
75870:+		h += hlen;
75871:+		while (*h == ' ' || *h == '\t')
75872:+			h++;
75873:+		/*
75874:+		 * Terminate token search on NULL or EOL.
75875:+		 */
75876:+		while (*h != 0 && *h != '\r' && *h != '\n') {
75877:+			if (strncasecmp(h, value, vlen) == 0)
75878:+				if (strchr(eov, h[vlen]) != NULL)
75879:+					return (ISC_TRUE);
75880:+			/*
75881:+			 * Skip to next token.
75882:+			 */
75883:+			h += strcspn(h, eov);
75884:+			if (h[0] == '\r' && h[1] == '\n')
75885:+				h++;
75886:+			if (h[0] != 0)
75887:+				h++;
75888:+		}
75889:+		return (ISC_FALSE);
75890:+	}
75891:+}
