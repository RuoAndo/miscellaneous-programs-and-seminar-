23909:+	test ${ttl2:-1} -eq 0 && break
23910:+	test ${ttl2:-1} -ge ${ttl1:-0} && break
23911:+	$PERL -e 'select(undef, undef, undef, 0.05);'
23912:+done
23913:+test ${ttl2:-1} -eq 0 && ret=1
23914:+test ${ttl2:-1} -ge ${ttl1:-0} || break
