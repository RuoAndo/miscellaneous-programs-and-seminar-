76923:+ATF_TC(isc_hash_function);
76924:+ATF_TC_HEAD(isc_hash_function, tc) {
76925:+	atf_tc_set_md_var(tc, "descr", "Hash function test");
76926:+}
76927:+ATF_TC_BODY(isc_hash_function, tc) {
76928:+	unsigned int h1;
76929:+	unsigned int h2;
76930:+
76931:+	UNUSED(tc);
76932:+
76933:+	/* Incremental hashing */
76934:+
76935:+	h1 = isc_hash_function(NULL, 0, ISC_TRUE, NULL);
76936:+	h1 = isc_hash_function("This ", 5, ISC_TRUE, &h1);
76937:+	h1 = isc_hash_function("is ", 3, ISC_TRUE, &h1);
76938:+	h1 = isc_hash_function("a long test", 12, ISC_TRUE, &h1);
76939:+
76940:+	h2 = isc_hash_function("This is a long test", 20,
76941:+			       ISC_TRUE, NULL);
76942:+
76943:+	ATF_CHECK_EQ(h1, h2);
76944:+
76945:+	/* Immutability of hash function */
76946:+	h1 = isc_hash_function(NULL, 0, ISC_TRUE, NULL);
76947:+	h2 = isc_hash_function(NULL, 0, ISC_TRUE, NULL);
76948:+
76949:+	ATF_CHECK_EQ(h1, h2);
76950:+
76951:+	/* Hash function characteristics */
76952:+	h1 = isc_hash_function("Hello world", 12, ISC_TRUE, NULL);
76953:+	h2 = isc_hash_function("Hello world", 12, ISC_TRUE, NULL);
76954:+
76955:+	ATF_CHECK_EQ(h1, h2);
76956:+
76957:+	/* Case */
76958:+	h1 = isc_hash_function("Hello world", 12, ISC_FALSE, NULL);
76959:+	h2 = isc_hash_function("heLLo WorLd", 12, ISC_FALSE, NULL);
76960:+
76961:+	ATF_CHECK_EQ(h1, h2);
76962:+
76963:+	/* Unequal */
76964:+	h1 = isc_hash_function("Hello world", 12, ISC_TRUE, NULL);
76965:+	h2 = isc_hash_function("heLLo WorLd", 12, ISC_TRUE, NULL);
76966:+
76967:+	ATF_CHECK(h1 != h2);
76968:+}
76969:+
