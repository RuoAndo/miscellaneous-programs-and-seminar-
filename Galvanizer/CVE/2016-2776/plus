73037:@@ -339,6 +417,160 @@
73041:+static inline isc_result_t
73042:+typemap_fromtext(isc_lex_t *lexer, isc_buffer_t *target,
73043:+		 isc_boolean_t allow_empty)
73044:+{
73045:+	isc_token_t token;
73046:+	unsigned char bm[8*1024]; /* 64k bits */
73047:+	dns_rdatatype_t covered, max_used;
73048:+	int octet;
73049:+	unsigned int max_octet, newend, end;
73050:+	int window;
73051:+	isc_boolean_t first = ISC_TRUE;
73052:+
73053:+	max_used = 0;
73054:+	bm[0] = 0;
73055:+	end = 0;
73056:+
73057:+	do {
73058:+		RETERR(isc_lex_getmastertoken(lexer, &token,
73059:+					      isc_tokentype_string, ISC_TRUE));
73060:+		if (token.type != isc_tokentype_string)
73061:+			break;
73062:+		RETTOK(dns_rdatatype_fromtext(&covered,
73063:+					      &token.value.as_textregion));
73064:+		if (covered > max_used) {
73065:+			newend = covered / 8;
73066:+			if (newend > end) {
73067:+				memset(&bm[end + 1], 0, newend - end);
73068:+				end = newend;
73069:+			}
73070:+			max_used = covered;
73071:+		}
73072:+		bm[covered/8] |= (0x80>>(covered%8));
73073:+		first = ISC_FALSE;
73074:+	} while (1);
73075:+	isc_lex_ungettoken(lexer, &token);
73076:+	if (!allow_empty && first)
73077:+		return (DNS_R_FORMERR);
73078:+
73079:+	for (window = 0; window < 256 ; window++) {
73080:+		if (max_used < window * 256)
73081:+			break;
73082:+
73083:+		max_octet = max_used - (window * 256);
73084:+		if (max_octet >= 256)
73085:+			max_octet = 31;
73086:+		else
73087:+			max_octet /= 8;
73088:+
73089:+		/*
73090:+		 * Find if we have a type in this window.
73091:+		 */
73092:+		for (octet = max_octet; octet >= 0; octet--) {
73093:+			if (bm[window * 32 + octet] != 0)
73094:+				break;
73095:+		}
73096:+		if (octet < 0)
73097:+			continue;
73098:+		RETERR(uint8_tobuffer(window, target));
73099:+		RETERR(uint8_tobuffer(octet + 1, target));
73100:+		RETERR(mem_tobuffer(target, &bm[window * 32], octet + 1));
73101:+	}
73102:+	return (ISC_R_SUCCESS);
73103:+}
73104:+
73105:+static inline isc_result_t
73106:+typemap_totext(isc_region_t *sr, dns_rdata_textctx_t *tctx,
73107:+	       isc_buffer_t *target)
73108:+{
73109:+	unsigned int i, j, k;
73110:+	unsigned int window, len;
73111:+	isc_boolean_t first = ISC_FALSE;
73112:+
73113:+	for (i = 0; i < sr->length; i += len) {
73114:+		if (tctx != NULL &&
73115:+		    (tctx->flags & DNS_STYLEFLAG_MULTILINE) != 0) {
73116:+			RETERR(str_totext(tctx->linebreak, target));
73117:+			first = ISC_TRUE;
73118:+		}
73119:+		INSIST(i + 2 <= sr->length);
73120:+		window = sr->base[i];
73121:+		len = sr->base[i + 1];
73122:+		INSIST(len > 0 && len <= 32);
73123:+		i += 2;
73124:+		INSIST(i + len <= sr->length);
73125:+		for (j = 0; j < len; j++) {
73126:+			dns_rdatatype_t t;
73127:+			if (sr->base[i + j] == 0)
73128:+				continue;
73129:+			for (k = 0; k < 8; k++) {
73130:+				if ((sr->base[i + j] & (0x80 >> k)) == 0)
73131:+					continue;
73132:+				t = window * 256 + j * 8 + k;
73133:+				if (!first)
73134:+					RETERR(str_totext(" ", target));
73135:+				first = ISC_FALSE;
73136:+				if (dns_rdatatype_isknown(t)) {
73137:+					RETERR(dns_rdatatype_totext(t, target));
73138:+				} else {
73139:+					char buf[sizeof("TYPE65535")];
73140:+					sprintf(buf, "TYPE%u", t);
73141:+					RETERR(str_totext(buf, target));
73142:+				}
73143:+			}
73144:+		}
73145:+	}
73146:+	return (ISC_R_SUCCESS);
73147:+}
73148:+
73149:+static isc_result_t
73150:+typemap_test(isc_region_t *sr, isc_boolean_t allow_empty) {
73151:+	unsigned int window, lastwindow = 0;
73152:+	unsigned int len;
73153:+	isc_boolean_t first = ISC_TRUE;
73154:+	unsigned int i;
73155:+
73156:+	for (i = 0; i < sr->length; i += len) {
73157:+		/*
73158:+		 * Check for overflow.
73159:+		 */
73160:+		if (i + 2 > sr->length)
73161:+			RETERR(DNS_R_FORMERR);
73162:+		window = sr->base[i];
73163:+		len = sr->base[i + 1];
73164:+		i += 2;
73165:+		/*
73166:+		 * Check that bitmap windows are in the correct order.
73167:+		 */
73168:+		if (!first && window <= lastwindow)
73169:+			RETERR(DNS_R_FORMERR);
73170:+		/*
73171:+		 * Check for legal lengths.
73172:+		 */
73173:+		if (len < 1 || len > 32)
73174:+			RETERR(DNS_R_FORMERR);
73175:+		/*
73176:+		 * Check for overflow.
73177:+		 */
73178:+		if (i + len > sr->length)
73179:+			RETERR(DNS_R_FORMERR);
73180:+		/*
73181:+		 * The last octet of the bitmap must be non zero.
73182:+		 */
73183:+		if (sr->base[i + len - 1] == 0)
73184:+			RETERR(DNS_R_FORMERR);
73185:+		lastwindow = window;
73186:+		first = ISC_FALSE;
73187:+	}
73188:+	if (i != sr->length)
73189:+		return (DNS_R_EXTRADATA);
73190:+	if (!allow_empty && first)
73191:+		RETERR(DNS_R_FORMERR);
73192:+	return (ISC_R_SUCCESS);
73193:+}
