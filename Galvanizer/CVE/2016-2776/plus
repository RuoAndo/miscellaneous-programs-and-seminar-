75640:@@ -399,3 +399,152 @@
75644:+
75645:+static isc_uint32_t fnv_offset_basis;
75646:+static isc_once_t fnv_once = ISC_ONCE_INIT;
75647:+
75648:+static void
75649:+fnv_initialize(void) {
75650:+	/*
75651:+	 * This function should not leave fnv_offset_basis set to
75652:+	 * 0. Also, after this function has been called, if it is called
75653:+	 * again, it should not change fnv_offset_basis.
75654:+	 */
75655:+	while (fnv_offset_basis == 0) {
75656:+		isc_random_get(&fnv_offset_basis);
75657:+	}
75658:+}
75659:+
75660:+isc_uint32_t
75661:+isc_hash_function(const void *data, size_t length,
75662:+		  isc_boolean_t case_sensitive,
75663:+		  const isc_uint32_t *previous_hashp)
75664:+{
75665:+	isc_uint32_t hval;
75666:+	const unsigned char *bp;
75667:+	const unsigned char *be;
75668:+
75669:+	INSIST(data == NULL || length > 0);
75670:+	RUNTIME_CHECK(isc_once_do(&fnv_once, fnv_initialize) == ISC_R_SUCCESS);
75671:+
75672:+	hval = ISC_UNLIKELY(previous_hashp != NULL) ?
75673:+		*previous_hashp : fnv_offset_basis;
75674:+
75675:+	if (length == 0)
75676:+		return (hval);
75677:+
75678:+	bp = (const unsigned char *) data;
75679:+	be = bp + length;
75680:+
75681:+	/*
75682:+	 * Fowler-Noll-Vo FNV-1a hash function.
75683:+	 *
75684:+	 * NOTE: A random fnv_offset_basis is used by default to avoid
75685:+	 * collision attacks as the hash function is reversible. This
75686:+	 * makes the mapping non-deterministic, but the distribution in
75687:+	 * the domain is still uniform.
75688:+	 */
75689:+
75690:+	if (case_sensitive) {
75691:+		while (bp < be - 4) {
75692:+			hval ^= (isc_uint32_t) bp[0];
75693:+			hval *= 16777619;
75694:+			hval ^= (isc_uint32_t) bp[1];
75695:+			hval *= 16777619;
75696:+			hval ^= (isc_uint32_t) bp[2];
75697:+			hval *= 16777619;
75698:+			hval ^= (isc_uint32_t) bp[3];
75699:+			hval *= 16777619;
75700:+			bp += 4;
75701:+		}
75702:+		while (bp < be) {
75703:+			hval ^= (isc_uint32_t) *bp++;
75704:+			hval *= 16777619;
75705:+		}
75706:+	} else {
75707:+		while (bp < be - 4) {
75708:+			hval ^= (isc_uint32_t) maptolower[bp[0]];
75709:+			hval *= 16777619;
75710:+			hval ^= (isc_uint32_t) maptolower[bp[1]];
75711:+			hval *= 16777619;
75712:+			hval ^= (isc_uint32_t) maptolower[bp[2]];
75713:+			hval *= 16777619;
75714:+			hval ^= (isc_uint32_t) maptolower[bp[3]];
75715:+			hval *= 16777619;
75716:+			bp += 4;
75717:+		}
75718:+		while (bp < be) {
75719:+			hval ^= (isc_uint32_t) maptolower[*bp++];
75720:+			hval *= 16777619;
75721:+		}
75722:+	}
75723:+
75724:+	return (hval);
75725:+}
75726:+
75727:+isc_uint32_t
75728:+isc_hash_function_reverse(const void *data, size_t length,
75729:+			  isc_boolean_t case_sensitive,
75730:+			  const isc_uint32_t *previous_hashp)
75731:+{
75732:+	isc_uint32_t hval;
75733:+	const unsigned char *bp;
75734:+	const unsigned char *be;
75735:+
75736:+	INSIST(data == NULL || length > 0);
75737:+	RUNTIME_CHECK(isc_once_do(&fnv_once, fnv_initialize) == ISC_R_SUCCESS);
75738:+
75739:+	hval = ISC_UNLIKELY(previous_hashp != NULL) ?
75740:+		*previous_hashp : fnv_offset_basis;
75741:+
75742:+	if (length == 0)
75743:+		return (hval);
75744:+
75745:+	bp = (const unsigned char *) data;
75746:+	be = bp + length;
75747:+
75748:+	/*
75749:+	 * Fowler-Noll-Vo FNV-1a hash function.
75750:+	 *
75751:+	 * NOTE: A random fnv_offset_basis is used by default to avoid
75752:+	 * collision attacks as the hash function is reversible. This
75753:+	 * makes the mapping non-deterministic, but the distribution in
75754:+	 * the domain is still uniform.
75755:+	 */
75756:+
75757:+	if (case_sensitive) {
75758:+		while (be >= bp + 4) {
75759:+			be -= 4;
75760:+			hval ^= (isc_uint32_t) be[3];
75761:+			hval *= 16777619;
75762:+			hval ^= (isc_uint32_t) be[2];
75763:+			hval *= 16777619;
75764:+			hval ^= (isc_uint32_t) be[1];
75765:+			hval *= 16777619;
75766:+			hval ^= (isc_uint32_t) be[0];
75767:+			hval *= 16777619;
75768:+		}
75769:+		while (--be >= bp) {
75770:+			hval ^= (isc_uint32_t) *be;
75771:+			hval *= 16777619;
75772:+		}
75773:+	} else {
75774:+		while (be >= bp + 4) {
75775:+			be -= 4;
75776:+			hval ^= (isc_uint32_t) maptolower[be[3]];
75777:+			hval *= 16777619;
75778:+			hval ^= (isc_uint32_t) maptolower[be[2]];
75779:+			hval *= 16777619;
75780:+			hval ^= (isc_uint32_t) maptolower[be[1]];
75781:+			hval *= 16777619;
75782:+			hval ^= (isc_uint32_t) maptolower[be[0]];
75783:+			hval *= 16777619;
75784:+		}
75785:+		while (--be >= bp) {
75786:+			hval ^= (isc_uint32_t) maptolower[*be];
75787:+			hval *= 16777619;
75788:+		}
75789:+	}
75790:+
75791:+	return (hval);
75792:+}
75794:--- bind-9.9.8/lib/isc/hex.c	2015-09-09 11:23:50.000000000 +0900
75795:+++ bind-9.9.9-P3/lib/isc/hex.c	2016-09-09 11:47:21.000000000 +0900
75796:@@ -1,5 +1,5 @@
