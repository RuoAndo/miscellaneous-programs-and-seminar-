77737:+static __inline__ void
77738:+isc_atomic_storeq(isc_int64_t *p, isc_int64_t val) {
77739:+	__asm__ volatile(
77740:+#ifdef ISC_PLATFORM_USETHREADS
77741:+		/*
77742:+		 * xchg should automatically lock memory, but we add it
77743:+		 * explicitly just in case (it at least doesn't harm)
77744:+		 */
77745:+		"lock;"
77746:+#endif
77747:+
77748:+		"xchgq %1, %0"
77749:+		:
77750:+		: "r"(val), "m"(*p)
77751:+		: "memory");
77752:+}
77753:+#endif /* ISC_PLATFORM_HAVEATOMICSTOREQ */
77754:+
