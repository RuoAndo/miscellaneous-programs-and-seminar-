26507:+// InstallFlags: runvcredist and forwin64 options
26508:+void CBINDInstallDlg::ReadInstallFlags() {
26509:+	std::ifstream ff(m_currentDir + "\\InstallFlags");
26510:+	if (!ff) {
26511:+		throw(Exception(IDS_FILE_BAD, "InstallFlags", "can't open"));
26512:+	}
26513:+	while (!ff.eof()) {
26514:+		std::string line;
26515:+		getline(ff, line);
26516:+		if (line.compare("runvcredist") == 0)
26517:+			runvcredist = TRUE;
26518:+		else if (line.compare("forwin64") == 0)
26519:+			forwin64 = TRUE;
26520:+	}
26521:+}
26522:+
26523:+// InstallFiles: {filename-divt}*
26524:+//   destination: TBEW
26525:+//   importance: TNC
26526:+//   checkVer and withTools: TF (boolean)
26527:+void CBINDInstallDlg::ReadInstallFileList() {
26528:+	std::ifstream fl(m_currentDir + "\\InstallFiles");
26529:+	if (!fl) {
26530:+		throw(Exception(IDS_FILE_BAD, "InstallFiles", "can't open"));
26531:+	}
26532:+	while (!fl.eof()) {
26533:+		std::string line;
26534:+		getline(fl, line);
26535:+		if (line.empty())
26536:+			continue;
26537:+		if (line[0] == '#')
26538:+			continue;
26539:+		// zip -l adds spurious \r: remove trailing space chars
26540:+		size_t finish = line.find_last_not_of(" \t\r\n\t\v");
26541:+		if ((finish != std::string::npos) &&
26542:+		    (finish + 1 != line.size())) {
26543:+			line.erase(finish + 1);
26544:+		}
26545:+		size_t flags = line.find_last_of('-');
26546:+		if ((flags == std::string::npos) ||
26547:+		    (flags + 5 != line.size()))
26548:+			goto bad;
26549:+		std::string file = line.substr(0, flags);
26550:+		if (file.empty() || (file.size() > 127))
26551:+			goto bad;
26552:+		FileData entry;
26553:+		memmove(entry.filename, file.c_str(), file.size() + 1);
26554:+		switch (line[flags + 1]) {
26555:+		case 'T':
26556:+			entry.destination = FileData::TargetDir;
26557:+			break;
26558:+		case 'B':
26559:+			entry.destination = FileData::BinDir;
26560:+			break;
26561:+		case 'E':
26562:+			entry.destination = FileData::EtcDir;
26563:+			break;
26564:+		case 'W':
26565:+			entry.destination = FileData::WinSystem;
26566:+			break;
26567:+		default:
26568:+			goto bad;
26569:+		}
26570:+		switch (line[flags + 2]) {
26571:+		case 'T':
26572:+			entry.importance = FileData::Trivial;
26573:+			break;
26574:+		case 'N':
26575:+			entry.importance = FileData::Normal;
26576:+			break;
26577:+		case 'C':
26578:+			entry.importance = FileData::Critical;
26579:+			break;
26580:+		default:
26581:+			goto bad;
26582:+		}
26583:+		switch (line[flags + 3]) {
26584:+		case 'T':
26585:+			entry.checkVer = TRUE;
26586:+			break;
26587:+		case 'F':
26588:+			entry.checkVer = FALSE;
26589:+			break;
26590:+		default:
26591:+			goto bad;
26592:+		}
26593:+		switch (line[flags + 4]) {
26594:+		case 'T':
26595:+			entry.withTools = TRUE;
26596:+			break;
26597:+		case 'F':
26598:+			entry.withTools = FALSE;
26599:+			break;
26600:+		default:
26601:+			goto bad;
26602:+		}
26603:+		installFiles.push_back(entry);
26604:+	}
26605:+	return;
26606:+
26607:+bad:
26608:+	throw(Exception(IDS_FILE_BAD, "InstallFiles", "syntax error"));
26609:+}
