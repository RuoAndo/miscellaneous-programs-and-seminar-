35587:+	    q
35588:+	  }
35589:+	  /^X\(\/\).*/{
35590:+	    s//\1/
35591:+	    q
35592:+	  }
35593:+	  s/.*/./; q'`
35594:+      test -d "$as_dir" && break
35595:+    done
35596:+    test -z "$as_dirs" || eval "mkdir $as_dirs"
35597:+  } || test -d "$as_dir" || as_fn_error $? "cannot create directory $as_dir"
35599:+
35600:+} # as_fn_mkdir_p
35603:+  as_mkdir_p='mkdir -p "$as_dir"'
