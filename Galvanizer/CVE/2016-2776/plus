74517:+static void
74518:+error_callback(dns_rdatacallbacks_t *callbacks, const char *fmt, ...) {
74519:+	UNUSED(callbacks);
74520:+	UNUSED(fmt);
74521:+}
74522:+
74523:+static void
74524:+warn_callback(dns_rdatacallbacks_t *callbacks, const char *fmt, ...) {
74525:+	UNUSED(callbacks);
74526:+	UNUSED(fmt);
74527:+}
74528:+
74529:+ATF_TC(nsec);
74530:+ATF_TC_HEAD(nsec, tc) {
74531:+	atf_tc_set_md_var(tc, "descr", "nsec to/from text/wire");
74532:+}
74533:+ATF_TC_BODY(nsec, tc) {
74534:+	struct {
74535:+		const char *data;
74536:+		isc_boolean_t ok;
74537:+	} text_data[] = {
74538:+		{ "", ISC_FALSE },
74539:+		{ ".", ISC_FALSE },
74540:+		{ ". RRSIG", ISC_TRUE },
74541:+		{ NULL, ISC_FALSE },
74542:+	};
74543:+	struct {
74544:+		unsigned char data[64];
74545:+		size_t len;
74546:+		isc_boolean_t ok;
74547:+	} wire_data[] = {
74548:+		{ { 0x00 }, 0,  ISC_FALSE },
74549:+		{ { 0x00 }, 1, ISC_FALSE },
74550:+		{ { 0x00, 0x00 }, 2, ISC_FALSE },
74551:+		{ { 0x00, 0x00, 0x00 }, 3, ISC_FALSE },
74552:+		{ { 0x00, 0x00, 0x01, 0x02 }, 4, ISC_TRUE }
74553:+	};
74554:+	unsigned char buf1[1024], buf2[1024];
74555:+	isc_buffer_t source, target1, target2;
74556:+	isc_result_t result;
74557:+	size_t i;
74558:+	dns_rdataclass_t rdclass = dns_rdataclass_in;
74559:+	dns_rdatatype_t type = dns_rdatatype_nsec;
74560:+	isc_lex_t *lex = NULL;
74561:+	dns_rdatacallbacks_t callbacks;
74562:+	dns_rdata_nsec_t nsec;
74563:+	dns_decompress_t dctx;
74564:+
74565:+	UNUSED(tc);
74566:+
74567:+	result = dns_test_begin(NULL, ISC_FALSE);
74568:+	ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74569:+
74570:+	result = isc_lex_create(mctx, 64, &lex);
74571:+	ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74572:+
74573:+	dns_rdatacallbacks_init(&callbacks);
74574:+	callbacks.error = error_callback;
74575:+	callbacks.warn = warn_callback;
74576:+
74577:+	for (i = 0; text_data[i].data != NULL; i++) {
74578:+		size_t length = strlen(text_data[i].data);
74579:+		isc_buffer_constinit(&source, text_data[i].data, length);
74580:+		isc_buffer_add(&source, length);
74581:+		result = isc_lex_openbuffer(lex, &source);
74582:+		ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74583:+
74584:+		isc_buffer_init(&target1, buf1, sizeof(buf1));
74585:+
74586:+		result = dns_rdata_fromtext(NULL, rdclass, type, lex,
74587:+					    dns_rootname, 0, NULL, &target1,
74588:+					    &callbacks);
74589:+		if (text_data[i].ok)
74590:+			ATF_CHECK_EQ(result, ISC_R_SUCCESS);
74591:+		else
74592:+			ATF_CHECK(result != ISC_R_SUCCESS);
74593:+	}
74594:+	isc_lex_destroy(&lex);
74595:+
74596:+	for (i = 0; i < sizeof(wire_data)/sizeof(wire_data[0]); i++) {
74597:+		dns_rdata_t rdata = DNS_RDATA_INIT;
74598:+
74599:+		isc_buffer_init(&source, wire_data[i].data, wire_data[i].len);
74600:+		isc_buffer_add(&source, wire_data[i].len);
74601:+		isc_buffer_setactive(&source, wire_data[i].len);
74602:+		isc_buffer_init(&target1, buf1, sizeof(buf1));
74603:+		dns_decompress_init(&dctx, -1, DNS_DECOMPRESS_ANY);
74604:+		result = dns_rdata_fromwire(&rdata, rdclass, type, &source,
74605:+					    &dctx, 0, &target1);
74606:+		dns_decompress_invalidate(&dctx);
74607:+		if (wire_data[i].ok)
74608:+			ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74609:+		else
74610:+			ATF_REQUIRE(result != ISC_R_SUCCESS);
74611:+		if (result != ISC_R_SUCCESS)
74612:+			continue;
74613:+		result = dns_rdata_tostruct(&rdata, &nsec, NULL);
74614:+		ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74615:+		isc_buffer_init(&target2, buf2, sizeof(buf2));
74616:+		dns_rdata_reset(&rdata);
74617:+		result = dns_rdata_fromstruct(&rdata, rdclass, type,
74618:+					      &nsec, &target2);
74619:+		ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74620:+		ATF_REQUIRE_EQ(isc_buffer_usedlength(&target2),
74621:+						     wire_data[i].len);
74622:+		ATF_REQUIRE_EQ(memcmp(buf2, wire_data[i].data,
74623:+				      wire_data[i].len), 0);
74624:+	}
74625:+}
74626:+
74627:+ATF_TC(csync);
74628:+ATF_TC_HEAD(csync, tc) {
74629:+	atf_tc_set_md_var(tc, "descr", "csync to/from struct");
74630:+}
74631:+ATF_TC_BODY(csync, tc) {
74632:+	struct {
74633:+		const char *data;
74634:+		isc_boolean_t ok;
74635:+	} text_data[] = {
74636:+		{ "", ISC_FALSE },
74637:+		{ "0", ISC_FALSE },
74638:+		{ "0 0", ISC_TRUE },
74639:+		{ "0 0 A", ISC_TRUE },
74640:+		{ "0 0 NS", ISC_TRUE },
74641:+		{ "0 0 AAAA", ISC_TRUE },
74642:+		{ "0 0 A AAAA", ISC_TRUE },
74643:+		{ "0 0 A NS AAAA", ISC_TRUE },
74644:+		{ "0 0 A NS AAAA BOGUS", ISC_FALSE },
74645:+		{ NULL, ISC_FALSE },
74646:+	};
74647:+	struct {
74648:+		unsigned char data[64];
74649:+		size_t len;
74650:+		isc_boolean_t ok;
74651:+	} wire_data[] = {
74652:+		/* short */
74653:+		{ { 0x00 }, 0,  ISC_FALSE },
74654:+		/* short */
74655:+		{ { 0x00 }, 1, ISC_FALSE },
74656:+		/* short */
74657:+		{ { 0x00, 0x00 }, 2, ISC_FALSE },
74658:+		/* short */
74659:+		{ { 0x00, 0x00, 0x00 }, 3, ISC_FALSE },
74660:+		/* short */
74661:+		{ { 0x00, 0x00, 0x00, 0x00 }, 4, ISC_FALSE },
74662:+		/* short */
74663:+		{ { 0x00, 0x00, 0x00, 0x00, 0x00 }, 5, ISC_FALSE },
74664:+		/* serial + flags only  */
74665:+		{ { 0x00, 0x00, 0x00, 0x00, 0x00, 0x00 }, 6, ISC_TRUE },
74666:+		/* bad type map */
74667:+		{ { 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00 }, 7, ISC_FALSE },
74668:+		/* bad type map */
74669:+		{ { 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00 },
74670:+		    8, ISC_FALSE },
74671:+		/* good type map */
74672:+		{ { 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x01, 0x02 },
74673:+		    9, ISC_TRUE }
74674:+	};
74675:+	unsigned char buf1[1024];
74676:+	unsigned char buf2[1024];
74677:+	isc_buffer_t source, target1, target2;
74678:+	isc_result_t result;
74679:+	size_t i;
74680:+	dns_rdataclass_t rdclass = dns_rdataclass_in;
74681:+	dns_rdatatype_t type = dns_rdatatype_csync;
74682:+	isc_lex_t *lex = NULL;
74683:+	dns_rdatacallbacks_t callbacks;
74684:+	dns_rdata_csync_t csync;
74685:+	dns_decompress_t dctx;
74686:+
74687:+	UNUSED(tc);
74688:+
74689:+	result = dns_test_begin(NULL, ISC_FALSE);
74690:+	ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74691:+
74692:+	result = isc_lex_create(mctx, 64, &lex);
74693:+	ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74694:+
74695:+	dns_rdatacallbacks_init(&callbacks);
74696:+	callbacks.error = error_callback;
74697:+	callbacks.warn = warn_callback;
74698:+
74699:+	for (i = 0; text_data[i].data != NULL; i++) {
74700:+		size_t length = strlen(text_data[i].data);
74701:+		isc_buffer_constinit(&source, text_data[i].data, length);
74702:+		isc_buffer_add(&source, length);
74703:+		result = isc_lex_openbuffer(lex, &source);
74704:+		ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74705:+
74706:+		isc_buffer_init(&target1, buf1, sizeof(buf1));
74707:+
74708:+		result = dns_rdata_fromtext(NULL, rdclass, type, lex,
74709:+					    dns_rootname, 0, NULL, &target1,
74710:+					    &callbacks);
74711:+		if (text_data[i].ok)
74712:+			ATF_CHECK_EQ(result, ISC_R_SUCCESS);
74713:+		else
74714:+			ATF_CHECK(result != ISC_R_SUCCESS);
74715:+	}
74716:+	isc_lex_destroy(&lex);
74717:+
74718:+	for (i = 0; i < sizeof(wire_data)/sizeof(wire_data[0]); i++) {
74719:+		dns_rdata_t rdata = DNS_RDATA_INIT;
74720:+
74721:+		isc_buffer_init(&source, wire_data[i].data, wire_data[i].len);
74722:+		isc_buffer_add(&source, wire_data[i].len);
74723:+		isc_buffer_setactive(&source, wire_data[i].len);
74724:+		isc_buffer_init(&target1, buf1, sizeof(buf1));
74725:+		dns_decompress_init(&dctx, -1, DNS_DECOMPRESS_ANY);
74726:+		result = dns_rdata_fromwire(&rdata, rdclass, type, &source,
74727:+					    &dctx, 0, &target1);
74728:+		dns_decompress_invalidate(&dctx);
74729:+		if (wire_data[i].ok)
74730:+			ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74731:+		else
74732:+			ATF_REQUIRE(result != ISC_R_SUCCESS);
74733:+		if (result != ISC_R_SUCCESS)
74734:+			continue;
74735:+		result = dns_rdata_tostruct(&rdata, &csync, NULL);
74736:+		ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74737:+		isc_buffer_init(&target2, buf2, sizeof(buf2));
74738:+		dns_rdata_reset(&rdata);
74739:+		result = dns_rdata_fromstruct(&rdata, rdclass, type,
74740:+					      &csync, &target2);
74741:+		ATF_REQUIRE_EQ(result, ISC_R_SUCCESS);
74742:+		ATF_REQUIRE_EQ(isc_buffer_usedlength(&target2),
74743:+						     wire_data[i].len);
74744:+		ATF_REQUIRE_EQ(memcmp(buf2, wire_data[i].data,
74745:+				      wire_data[i].len), 0);
