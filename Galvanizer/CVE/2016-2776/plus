87396:+# get the version information
87397:+
87398:+my %Versions;
87399:+
87400:+sub getversion {
87401:+    my $data;
87402:+    my $name;
87403:+    my $value;
87404:+    my $version;
87405:+
87406:+    open V, "..\\version" || die $!;
87407:+    while (<V>) {
87408:+        chomp;
87409:+        ($data) = split(/\#/);
87410:+        if ($data) {
87411:+            ($name, $value) = split(/=/, $data);
87412:+            ($name) = split(/\s+/, $name);
87413:+            if ($name eq 'PRODUCT' || $name eq 'DESCRIPTION') {
87414:+                ($value) =~ s/^["\s]+//;
87415:+                ($value) =~ s/["\s]+$//;
87416:+            } else {
87417:+                ($value) = split(/\s+/, $value);
87418:+            }
87419:+            $Versions{$name} = $value;
87420:+        }
87421:+    }
87422:+    close V;
87423:+
87424:+    $version = "$Versions{'MAJORVER'}.$Versions{'MINORVER'}";
87425:+    if ($Versions{'PATCHVER'} ne "") {
87426:+        $version = "$version.$Versions{'PATCHVER'}";
87427:+    }
87428:+    $version = "$version$Versions{'RELEASETYPE'}$Versions{'RELEASEVER'}";
87429:+    $version = "$version$Versions{'EXTENSIONS'}";
87430:+    $configvar{"BIND9_VERSION"} = "$version";
87431:+}
87432:+
87433:+getversion();
87434:+
