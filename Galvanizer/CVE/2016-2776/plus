28070:+	    s//\1/
28071:+	    q
28072:+	  }
28073:+	  /^X\(\/\/\)$/{
28074:+	    s//\1/
28075:+	    q
28076:+	  }
28077:+	  /^X\(\/\).*/{
28078:+	    s//\1/
28079:+	    q
28080:+	  }
28081:+	  s/.*/./; q'`
28082:+      test -d "$as_dir" && break
28083:+    done
28084:+    test -z "$as_dirs" || eval "mkdir $as_dirs"
28085:+  } || test -d "$as_dir" || as_fn_error $? "cannot create directory $as_dir"
28086:+
28087:+
28088:+} # as_fn_mkdir_p
