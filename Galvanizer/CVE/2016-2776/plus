29299:+# ac_fn_c_try_compile LINENO
29300:+# --------------------------
29301:+# Try to compile conftest.$ac_ext, and return whether this succeeded.
29302:+ac_fn_c_try_compile ()
29308:+  as_lineno=${as_lineno-"$1"} as_lineno_stack=as_lineno_stack=$as_lineno_stack
29309:+  rm -f conftest.$ac_objext
29310:+  if { { ac_try="$ac_compile"
29311:+case "(($ac_try" in
29312:+  *\"* | *\`* | *\\*) ac_try_echo=\$ac_try;;
29313:+  *) ac_try_echo=$ac_try;;
29314:+esac
29315:+eval ac_try_echo="\"\$as_me:${as_lineno-$LINENO}: $ac_try_echo\""
29316:+$as_echo "$ac_try_echo"; } >&5
29317:+  (eval "$ac_compile") 2>conftest.err
29318:+  ac_status=$?
29319:+  if test -s conftest.err; then
29320:+    grep -v '^ *+' conftest.err >conftest.er1
29321:+    cat conftest.er1 >&5
29322:+    mv -f conftest.er1 conftest.err
29323:+  fi
29324:+  $as_echo "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
29325:+  test $ac_status = 0; } && {
29326:+	 test -z "$ac_c_werror_flag" ||
29327:+	 test ! -s conftest.err
29328:+       } && test -s conftest.$ac_objext; then :
29329:+  ac_retval=0
29330:+else
29331:+  $as_echo "$as_me: failed program was:" >&5
29332:+sed 's/^/| /' conftest.$ac_ext >&5
29333:+
29334:+	ac_retval=1
29335:+fi
29336:+  eval $as_lineno_stack; ${as_lineno_stack:+:} unset as_lineno
29337:+  as_fn_set_status $ac_retval
29338:+
29339:+} # ac_fn_c_try_compile
29340:+
29341:+# ac_fn_c_try_link LINENO
29342:+# -----------------------
29343:+# Try to link conftest.$ac_ext, and return whether this succeeded.
29344:+ac_fn_c_try_link ()
29345:+{
29346:+  as_lineno=${as_lineno-"$1"} as_lineno_stack=as_lineno_stack=$as_lineno_stack
29347:+  rm -f conftest.$ac_objext conftest$ac_exeext
29348:+  if { { ac_try="$ac_link"
29349:+case "(($ac_try" in
29350:+  *\"* | *\`* | *\\*) ac_try_echo=\$ac_try;;
29351:+  *) ac_try_echo=$ac_try;;
29352:+esac
29353:+eval ac_try_echo="\"\$as_me:${as_lineno-$LINENO}: $ac_try_echo\""
29354:+$as_echo "$ac_try_echo"; } >&5
29355:+  (eval "$ac_link") 2>conftest.err
29356:+  ac_status=$?
29357:+  if test -s conftest.err; then
29358:+    grep -v '^ *+' conftest.err >conftest.er1
29359:+    cat conftest.er1 >&5
29360:+    mv -f conftest.er1 conftest.err
29361:+  fi
29362:+  $as_echo "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
29363:+  test $ac_status = 0; } && {
29364:+	 test -z "$ac_c_werror_flag" ||
29365:+	 test ! -s conftest.err
29366:+       } && test -s conftest$ac_exeext && {
29367:+	 test "$cross_compiling" = yes ||
29368:+	 test -x conftest$ac_exeext
29369:+       }; then :
29370:+  ac_retval=0
29371:+else
29372:+  $as_echo "$as_me: failed program was:" >&5
29373:+sed 's/^/| /' conftest.$ac_ext >&5
29374:+
29375:+	ac_retval=1
29376:+fi
29377:+  # Delete the IPA/IPO (Inter Procedural Analysis/Optimization) information
29378:+  # created by the PGI compiler (conftest_ipa8_conftest.oo), as it would
29379:+  # interfere with the next link command; also delete a directory that is
29380:+  # left behind by Apple's compiler.  We do this before executing the actions.
29381:+  rm -rf conftest.dSYM conftest_ipa8_conftest.oo
29382:+  eval $as_lineno_stack; ${as_lineno_stack:+:} unset as_lineno
29383:+  as_fn_set_status $ac_retval
29384:+
29385:+} # ac_fn_c_try_link
29386:+
29387:+# ac_fn_c_try_cpp LINENO
29388:+# ----------------------
29389:+# Try to preprocess conftest.$ac_ext, and return whether this succeeded.
29390:+ac_fn_c_try_cpp ()
29391:+{
29392:+  as_lineno=${as_lineno-"$1"} as_lineno_stack=as_lineno_stack=$as_lineno_stack
29393:+  if { { ac_try="$ac_cpp conftest.$ac_ext"
29394:+case "(($ac_try" in
29395:+  *\"* | *\`* | *\\*) ac_try_echo=\$ac_try;;
29396:+  *) ac_try_echo=$ac_try;;
29397:+esac
29398:+eval ac_try_echo="\"\$as_me:${as_lineno-$LINENO}: $ac_try_echo\""
29399:+$as_echo "$ac_try_echo"; } >&5
29400:+  (eval "$ac_cpp conftest.$ac_ext") 2>conftest.err
29401:+  ac_status=$?
29402:+  if test -s conftest.err; then
29403:+    grep -v '^ *+' conftest.err >conftest.er1
29404:+    cat conftest.er1 >&5
29405:+    mv -f conftest.er1 conftest.err
29406:+  fi
29407:+  $as_echo "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
29408:+  test $ac_status = 0; } > conftest.i && {
29409:+	 test -z "$ac_c_preproc_warn_flag$ac_c_werror_flag" ||
29410:+	 test ! -s conftest.err
29411:+       }; then :
29412:+  ac_retval=0
29413:+else
29414:+  $as_echo "$as_me: failed program was:" >&5
29415:+sed 's/^/| /' conftest.$ac_ext >&5
29416:+
29417:+    ac_retval=1
29418:+fi
29419:+  eval $as_lineno_stack; ${as_lineno_stack:+:} unset as_lineno
29420:+  as_fn_set_status $ac_retval
29421:+
29422:+} # ac_fn_c_try_cpp
29423:+
29424:+# ac_fn_c_try_run LINENO
29425:+# ----------------------
29426:+# Try to link conftest.$ac_ext, and return whether this succeeded. Assumes
29427:+# that executables *can* be run.
29428:+ac_fn_c_try_run ()
29429:+{
29430:+  as_lineno=${as_lineno-"$1"} as_lineno_stack=as_lineno_stack=$as_lineno_stack
29431:+  if { { ac_try="$ac_link"
29432:+case "(($ac_try" in
29433:+  *\"* | *\`* | *\\*) ac_try_echo=\$ac_try;;
29434:+  *) ac_try_echo=$ac_try;;
29435:+esac
29436:+eval ac_try_echo="\"\$as_me:${as_lineno-$LINENO}: $ac_try_echo\""
29437:+$as_echo "$ac_try_echo"; } >&5
29438:+  (eval "$ac_link") 2>&5
29439:+  ac_status=$?
29440:+  $as_echo "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
29441:+  test $ac_status = 0; } && { ac_try='./conftest$ac_exeext'
29442:+  { { case "(($ac_try" in
29443:+  *\"* | *\`* | *\\*) ac_try_echo=\$ac_try;;
29444:+  *) ac_try_echo=$ac_try;;
29445:+esac
29446:+eval ac_try_echo="\"\$as_me:${as_lineno-$LINENO}: $ac_try_echo\""
29447:+$as_echo "$ac_try_echo"; } >&5
29448:+  (eval "$ac_try") 2>&5
29449:+  ac_status=$?
29450:+  $as_echo "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
29451:+  test $ac_status = 0; }; }; then :
29452:+  ac_retval=0
29453:+else
29454:+  $as_echo "$as_me: program exited with status $ac_status" >&5
29455:+       $as_echo "$as_me: failed program was:" >&5
29456:+sed 's/^/| /' conftest.$ac_ext >&5
29457:+
29458:+       ac_retval=$ac_status
29459:+fi
29460:+  rm -rf conftest.dSYM conftest_ipa8_conftest.oo
29461:+  eval $as_lineno_stack; ${as_lineno_stack:+:} unset as_lineno
29462:+  as_fn_set_status $ac_retval
29463:+
29464:+} # ac_fn_c_try_run
29465:+
29466:+# ac_fn_c_check_header_mongrel LINENO HEADER VAR INCLUDES
29467:+# -------------------------------------------------------
29468:+# Tests whether HEADER exists, giving a warning if it cannot be compiled using
29469:+# the include files in INCLUDES and setting the cache variable VAR
29470:+# accordingly.
29471:+ac_fn_c_check_header_mongrel ()
29472:+{
29473:+  as_lineno=${as_lineno-"$1"} as_lineno_stack=as_lineno_stack=$as_lineno_stack
29474:+  if eval \${$3+:} false; then :
29475:+  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for $2" >&5
29476:+$as_echo_n "checking for $2... " >&6; }
29477:+if eval \${$3+:} false; then :
29478:+  $as_echo_n "(cached) " >&6
29479:+fi
29480:+eval ac_res=\$$3
29481:+	       { $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_res" >&5
29482:+$as_echo "$ac_res" >&6; }
29483:+else
29484:+  # Is the header compilable?
29485:+{ $as_echo "$as_me:${as_lineno-$LINENO}: checking $2 usability" >&5
29486:+$as_echo_n "checking $2 usability... " >&6; }
29487:+cat confdefs.h - <<_ACEOF >conftest.$ac_ext
29488:+/* end confdefs.h.  */
29489:+$4
29490:+#include <$2>
29491:+_ACEOF
29492:+if ac_fn_c_try_compile "$LINENO"; then :
29493:+  ac_header_compiler=yes
29494:+else
29495:+  ac_header_compiler=no
29496:+fi
29497:+rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
29498:+{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_header_compiler" >&5
29499:+$as_echo "$ac_header_compiler" >&6; }
29506:+# Is the header present?
29507:+{ $as_echo "$as_me:${as_lineno-$LINENO}: checking $2 presence" >&5
29508:+$as_echo_n "checking $2 presence... " >&6; }
29509:+cat confdefs.h - <<_ACEOF >conftest.$ac_ext
29510:+/* end confdefs.h.  */
29511:+#include <$2>
29512:+_ACEOF
29513:+if ac_fn_c_try_cpp "$LINENO"; then :
29514:+  ac_header_preproc=yes
29515:+else
29516:+  ac_header_preproc=no
29517:+fi
29518:+rm -f conftest.err conftest.i conftest.$ac_ext
29519:+{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_header_preproc" >&5
29520:+$as_echo "$ac_header_preproc" >&6; }
29524:+# So?  What about this header?
29525:+case $ac_header_compiler:$ac_header_preproc:$ac_c_preproc_warn_flag in #((
29526:+  yes:no: )
29527:+    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: $2: accepted by the compiler, rejected by the preprocessor!" >&5
29528:+$as_echo "$as_me: WARNING: $2: accepted by the compiler, rejected by the preprocessor!" >&2;}
29529:+    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: $2: proceeding with the compiler's result" >&5
29530:+$as_echo "$as_me: WARNING: $2: proceeding with the compiler's result" >&2;}
29531:+    ;;
29532:+  no:yes:* )
29533:+    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: $2: present but cannot be compiled" >&5
29534:+$as_echo "$as_me: WARNING: $2: present but cannot be compiled" >&2;}
29535:+    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: $2:     check for missing prerequisite headers?" >&5
29536:+$as_echo "$as_me: WARNING: $2:     check for missing prerequisite headers?" >&2;}
29537:+    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: $2: see the Autoconf documentation" >&5
29538:+$as_echo "$as_me: WARNING: $2: see the Autoconf documentation" >&2;}
29539:+    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: $2:     section \"Present But Cannot Be Compiled\"" >&5
29540:+$as_echo "$as_me: WARNING: $2:     section \"Present But Cannot Be Compiled\"" >&2;}
29541:+    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: $2: proceeding with the compiler's result" >&5
29542:+$as_echo "$as_me: WARNING: $2: proceeding with the compiler's result" >&2;}
29543:+    ;;
29544:+esac
29545:+  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for $2" >&5
29546:+$as_echo_n "checking for $2... " >&6; }
29547:+if eval \${$3+:} false; then :
29548:+  $as_echo_n "(cached) " >&6
29549:+else
29550:+  eval "$3=\$ac_header_compiler"
29551:+fi
29552:+eval ac_res=\$$3
29553:+	       { $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_res" >&5
29554:+$as_echo "$ac_res" >&6; }
29555:+fi
29556:+  eval $as_lineno_stack; ${as_lineno_stack:+:} unset as_lineno
29565:+} # ac_fn_c_check_header_mongrel
29566:+
29567:+# ac_fn_c_check_header_compile LINENO HEADER VAR INCLUDES
29568:+# -------------------------------------------------------
29569:+# Tests whether HEADER exists and can be compiled using the include files in
29570:+# INCLUDES, setting the cache variable VAR accordingly.
29571:+ac_fn_c_check_header_compile ()
29572:+{
29573:+  as_lineno=${as_lineno-"$1"} as_lineno_stack=as_lineno_stack=$as_lineno_stack
29574:+  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for $2" >&5
29575:+$as_echo_n "checking for $2... " >&6; }
29576:+if eval \${$3+:} false; then :
29577:+  $as_echo_n "(cached) " >&6
29578:+else
29579:+  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
29580:+/* end confdefs.h.  */
29581:+$4
29582:+#include <$2>
29583:+_ACEOF
29584:+if ac_fn_c_try_compile "$LINENO"; then :
29585:+  eval "$3=yes"
29586:+else
29587:+  eval "$3=no"
29588:+fi
29589:+rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
29590:+fi
29591:+eval ac_res=\$$3
29592:+	       { $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_res" >&5
29593:+$as_echo "$ac_res" >&6; }
29594:+  eval $as_lineno_stack; ${as_lineno_stack:+:} unset as_lineno
29595:+
29596:+} # ac_fn_c_check_header_compile
29597:+
29598:+# ac_fn_c_compute_int LINENO EXPR VAR INCLUDES
29599:+# --------------------------------------------
29600:+# Tries to find the compile-time value of EXPR in a program that includes
29601:+# INCLUDES, setting VAR accordingly. Returns whether the value could be
29602:+# computed
29603:+ac_fn_c_compute_int ()
29604:+{
29605:+  as_lineno=${as_lineno-"$1"} as_lineno_stack=as_lineno_stack=$as_lineno_stack
29606:+  if test "$cross_compiling" = yes; then
29607:+    # Depending upon the size, compute the lo and hi bounds.
29608:+cat confdefs.h - <<_ACEOF >conftest.$ac_ext
29609:+/* end confdefs.h.  */
29610:+$4
29611:+int
29612:+main ()
29613:+{
29614:+static int test_array [1 - 2 * !(($2) >= 0)];
29615:+test_array [0] = 0;
29616:+return test_array [0];
29617:+
29618:+  ;
29619:+  return 0;
29620:+}
29621:+_ACEOF
29622:+if ac_fn_c_try_compile "$LINENO"; then :
29623:+  ac_lo=0 ac_mid=0
29624:+  while :; do
29625:+    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
29626:+/* end confdefs.h.  */
29627:+$4
29628:+int
29629:+main ()
29630:+{
29631:+static int test_array [1 - 2 * !(($2) <= $ac_mid)];
29632:+test_array [0] = 0;
29633:+return test_array [0];
29634:+
29635:+  ;
29636:+  return 0;
29637:+}
29638:+_ACEOF
29639:+if ac_fn_c_try_compile "$LINENO"; then :
29640:+  ac_hi=$ac_mid; break
29641:+else
29642:+  as_fn_arith $ac_mid + 1 && ac_lo=$as_val
29643:+			if test $ac_lo -le $ac_mid; then
29644:+			  ac_lo= ac_hi=
29645:+			  break
29646:+			fi
29647:+			as_fn_arith 2 '*' $ac_mid + 1 && ac_mid=$as_val
29648:+fi
29649:+rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
29650:+  done
29651:+else
29652:+  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
29653:+/* end confdefs.h.  */
29654:+$4
29655:+int
29656:+main ()
29657:+{
29658:+static int test_array [1 - 2 * !(($2) < 0)];
29659:+test_array [0] = 0;
29660:+return test_array [0];
29661:+
29662:+  ;
29663:+  return 0;
29664:+}
29665:+_ACEOF
29666:+if ac_fn_c_try_compile "$LINENO"; then :
29667:+  ac_hi=-1 ac_mid=-1
29668:+  while :; do
29669:+    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
29670:+/* end confdefs.h.  */
29671:+$4
29672:+int
29673:+main ()
29674:+{
29675:+static int test_array [1 - 2 * !(($2) >= $ac_mid)];
29676:+test_array [0] = 0;
29677:+return test_array [0];
29678:+
29679:+  ;
29680:+  return 0;
29681:+}
29682:+_ACEOF
29683:+if ac_fn_c_try_compile "$LINENO"; then :
29684:+  ac_lo=$ac_mid; break
29685:+else
29686:+  as_fn_arith '(' $ac_mid ')' - 1 && ac_hi=$as_val
29687:+			if test $ac_mid -le $ac_hi; then
29688:+			  ac_lo= ac_hi=
29689:+			  break
29690:+			fi
29691:+			as_fn_arith 2 '*' $ac_mid && ac_mid=$as_val
29692:+fi
29693:+rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
29694:+  done
29695:+else
29696:+  ac_lo= ac_hi=
29697:+fi
29698:+rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
29699:+fi
29700:+rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
29701:+# Binary search between lo and hi bounds.
29702:+while test "x$ac_lo" != "x$ac_hi"; do
29703:+  as_fn_arith '(' $ac_hi - $ac_lo ')' / 2 + $ac_lo && ac_mid=$as_val
29704:+  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
29705:+/* end confdefs.h.  */
29706:+$4
29707:+int
29708:+main ()
29709:+{
29710:+static int test_array [1 - 2 * !(($2) <= $ac_mid)];
29711:+test_array [0] = 0;
29712:+return test_array [0];
29713:+
29714:+  ;
29715:+  return 0;
29716:+}
29717:+_ACEOF
29718:+if ac_fn_c_try_compile "$LINENO"; then :
29719:+  ac_hi=$ac_mid
29720:+else
29721:+  as_fn_arith '(' $ac_mid ')' + 1 && ac_lo=$as_val
29722:+fi
29723:+rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
29724:+done
29725:+case $ac_lo in #((
29726:+?*) eval "$3=\$ac_lo"; ac_retval=0 ;;
29727:+'') ac_retval=1 ;;
29728:+esac
29729:+  else
29730:+    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
29731:+/* end confdefs.h.  */
29732:+$4
29733:+static long int longval () { return $2; }
29734:+static unsigned long int ulongval () { return $2; }
29735:+#include <stdio.h>
29736:+#include <stdlib.h>
29737:+int
29738:+main ()
29739:+{
29740:+
29741:+  FILE *f = fopen ("conftest.val", "w");
29742:+  if (! f)
29743:+    return 1;
29744:+  if (($2) < 0)
29745:+    {
29746:+      long int i = longval ();
29747:+      if (i != ($2))
29748:+	return 1;
29749:+      fprintf (f, "%ld", i);
29750:+    }
29751:+  else
29752:+    {
29753:+      unsigned long int i = ulongval ();
29754:+      if (i != ($2))
29755:+	return 1;
29756:+      fprintf (f, "%lu", i);
29757:+    }
29758:+  /* Do not output a trailing newline, as this causes \r\n confusion
29759:+     on some platforms.  */
29760:+  return ferror (f) || fclose (f) != 0;
29761:+
29762:+  ;
29763:+  return 0;
29764:+}
29765:+_ACEOF
29766:+if ac_fn_c_try_run "$LINENO"; then :
29767:+  echo >>conftest.val; read $3 <conftest.val; ac_retval=0
29768:+else
29769:+  ac_retval=1
29770:+fi
29771:+rm -f core *.core core.conftest.* gmon.out bb.out conftest$ac_exeext \
29772:+  conftest.$ac_objext conftest.beam conftest.$ac_ext
29773:+rm -f conftest.val
29774:+
29775:+  fi
29776:+  eval $as_lineno_stack; ${as_lineno_stack:+:} unset as_lineno
29777:+  as_fn_set_status $ac_retval
29778:+
29779:+} # ac_fn_c_compute_int
29780:+cat >config.log <<_ACEOF
29781:+This file contains any messages produced by compilers while
29782:+running configure, to aid debugging if configure makes a mistake.
29783:+
29784:+It was created by $as_me, which was
29785:+generated by GNU Autoconf 2.69.  Invocation command line was
29786:+
29787:+  $ $0 $@
29788:+
29789:+_ACEOF
29790:+exec 5>>config.log
29791:+{
29792:+cat <<_ASUNAME
29793:+## --------- ##
29794:+## Platform. ##
29795:+## --------- ##
29796:+
29797:+hostname = `(hostname || uname -n) 2>/dev/null | sed 1q`
29798:+uname -m = `(uname -m) 2>/dev/null || echo unknown`
