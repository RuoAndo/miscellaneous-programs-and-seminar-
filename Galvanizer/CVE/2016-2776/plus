21849:+		tasks[i] = NULL;
21850:+		CHECK(isc_task_create(task_manager, 0, &tasks[i]));
21851:+		event = isc_event_allocate(mctx, NULL, 1000, do_storeq,
21852:+					   &counters[i], sizeof(struct isc_event));
21853:+		isc_task_sendanddetach(&tasks[i], &event);
21854:+	}
21855:+
21856:+	teardown();
21857:+
21858:+	test_result = T_PASS;
21859:+	r = store_64 & 0xff;
21860:+	val = (((isc_uint64_t) r << 24) |
21861:+	       ((isc_uint64_t) r << 16) |
21862:+	       ((isc_uint64_t) r << 8) |
21863:+	       (isc_uint64_t) r);
21864:+	val |= ((isc_uint64_t) val << 32);
21865:+	t_info("64-bit store 0x%"ISC_PRINT_QUADFORMAT"x, expected 0x%"ISC_PRINT_QUADFORMAT"x\n",
21866:+	       (isc_uint64_t) store_64, val);
21867:+	if ((isc_uint64_t) store_64 != val)
21868:+		test_result = T_FAIL;
21869:+	t_result(test_result);
