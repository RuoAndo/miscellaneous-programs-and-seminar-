69203:- * + deleting everything DOWN from a node that is itself being deleted, and
69204:- * + deleting the entire tree of trees from dns_rbt_destroy.
69249:+static void
69250:+freenode(dns_rbt_t *rbt, dns_rbtnode_t **nodep) {
69251:+	dns_rbtnode_t *node = *nodep;
69254:+	*nodep = NULL;
69255:+
69263:+deletetreeflat(dns_rbt_t *rbt, unsigned int quantum, isc_boolean_t unhash,
69264:+	       dns_rbtnode_t **nodep)
69285:+	dns_rbtnode_t *root = *nodep;
69289:+	while (root != NULL) {
69290:+		/*
69291:+		 * If there is a left, right or down node, walk into it
69292:+		 * and iterate.
69293:+		 */
69294:+		if (LEFT(root) != NULL) {
69295:+			dns_rbtnode_t *node = root;
69296:+			root = LEFT(root);
69297:+			LEFT(node) = NULL;
69298:+		} else if (RIGHT(root) != NULL) {
69299:+			dns_rbtnode_t *node = root;
69300:+			root = RIGHT(root);
69301:+			RIGHT(node) = NULL;
69302:+		} else if (DOWN(root) != NULL) {
69303:+			dns_rbtnode_t *node = root;
69304:+			root = DOWN(root);
69305:+			DOWN(node) = NULL;
69306:+		} else {
69307:+			/*
69308:+			 * There are no left, right or down nodes, so we
69309:+			 * can free this one and go back to its parent.
69310:+			 */
69311:+			dns_rbtnode_t *node = root;
69312:+			root = PARENT(root);
69318:+			if (DATA(node) != NULL && rbt->data_deleter != NULL)
69319:+				rbt->data_deleter(DATA(node),
69320:+						  rbt->deleter_arg);
69321:+			if (unhash)
69322:+				unhash_node(rbt, node);
69323:+			/*
69324:+			 * Note: we don't call unhash_node() here as we
69325:+			 * are destroying the complete RBT tree.
69326:+			 */
69329:+			node->magic = 0;
69348:+			freenode(rbt, &node);
69349:+			if (quantum != 0 && --quantum == 0)
69350:+				break;
69351:+		}
69354:+
69355:+	*nodep = root;
